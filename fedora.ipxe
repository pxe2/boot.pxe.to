:older_release
set older_release true
set fedora_mirror old-releases.fedora.com
echo Setting mirror to ${fedora_mirror}
clear fedora_version
echo -n Please set enter code name of release: ${} && read fedora_version
set dir ${fedora_base_dir}/dists/${fedora_version}/main/installer-${arch_a}/current/images/netboot/
goto fedora_boot_type

:mirrorcfg
set mirrorcfg mirror/suite=${fedora_version}
set dir ${fedora_base_dir}/dists/${fedora_version}/main/installer-${arch_a}/current/images/netboot/

:fedora_boot_type
menu ${os} [${fedora_version}] Installer
item --gap Install types
item install ${space} Install
item rescue ${space} Rescue Mode
item expert ${space} Expert Install
item preseed ${space} Specify preseed url...
choose --default ${type} type || goto fedora
echo ${cls}
goto fedora_${type}

:fedora_rescue
set install_params rescue/enable=true
goto fedora_boot

:fedora_expert
set install_params priority=low
goto fedora_boot

:fedora_preseed
echo -n Specify preseed URL for ${os} ${fedora_version}: && read preseedurl
set install_params auto=true priority=critical preseed/url=${preseedurl}
goto fedora_boot

:fedora_install
:fedora_boot
set dir ${dir}${menu}-installer/${arch_a}
imgfree
kernel http://${fedora_mirror}/${dir}/linux ${install_params} ${netcfg} ${mirrorcfg} ${console} -- quiet ${params} initrd=initrd.gz
initrd http://${fedora_mirror}/${dir}/initrd.gz
echo
echo MD5sums:
md5sum linux initrd.gz
iseq ${img_sigs_enabled} true && iseq ${older_release} true && goto skip_sigs ||
iseq ${img_sigs_enabled} true && goto verify_sigs || goto skip_sigs
:verify_sigs
echo
echo Checking signatures...
imgverify linux ${sigs}${dir}/linux.sig || goto error
imgverify initrd.gz ${sigs}${dir}/initrd.gz.sig || goto error
echo Signatures verified!
echo
:skip_sigs
boot

:fedora_exit
clear menu
exit 0
